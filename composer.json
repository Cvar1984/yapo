{
  "name": "cvar1984/yapo",
  "description": "Yet another php obfuscator",
  "type": "application",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Cvar1984",
      "email": "Cvar1984@pm.me",
      "role": "developer"
    }
  ],
  "bin": [
    "bin/yapo"
  ],
  "autoload": {
    "psr-4": {
      "Cvar1984\\":"src/"
    }
  },
  "require": {
    "symfony/console": "4.*"
  },
  "require-dev": {
    "humbug/box": "^3.8"
  },
  "scripts": {
    "make": "./vendor/bin/box compile",
    "check": "./vendor/bin/box verify ./bin/yapo"
  },
  "config": {
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true,
      "ocramius/package-versions": true
    }
  }
}
